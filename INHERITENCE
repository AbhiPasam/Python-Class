class Calculator:
    def __init__(self):
        print("Creating the Scientific Calculator")
    def add(self,a,b):
        return a+b
class scientificCalculator(Calculator):
    def __init__(self):
        print("Creating an object for the Scientific Calculator")
    def exp(self,a,b):
        return a**b
exp1 = scientificCalculator()
add_result=exp1.add(2,3)
exp_result = exp1.exp(3,4)
print(add_result,exp_result)
